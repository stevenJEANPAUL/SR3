{% extends 'base-backend.html.twig' %}

{% block title %}Rncp{% endblock %}

{% block body %}
    <h1>Rncp</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rncp.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ rncp.titre }}</td>
            </tr>
            <tr>
                <th>Formation</th>
                <td>{{ rncp.formation }}</td>
            </tr>
            <tr>
                <th>Niveau</th>
                <td>{{ rncp.niveau }}</td>
            </tr>
            <tr>
                <th>Descriptif</th>
                <td>{{ rncp.descriptif }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ rncp.imageName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ rncp.updatedAt ? rncp.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_rncp_index') }}">back to list</a>

    <a href="{{ path('app_admin_rncp_edit', {'id': rncp.id}) }}">edit</a>

    {{ include('admin_rncp/_delete_form.html.twig') }}
{% endblock %}
